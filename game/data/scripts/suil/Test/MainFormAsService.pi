	function OnWheelTextbox(_sender, _event, _params)
	{
		_offset = 5.0f;
		if (_params[0] < 0)
			_offset = -5.0f;

		_scroll = _sender.GetVerticalScroll() + _offset / 100.0f;
		if (_scroll < 0) _scroll = 0;
		if (_scroll > 1) _scroll = 1;
		_sender.ScrollVertical(_scroll);
	}



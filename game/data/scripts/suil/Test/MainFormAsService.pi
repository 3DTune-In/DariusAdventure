	function CreateFrame(_texture, _name, _texName = null)
	{
		_f2 = UI_CreateFrame("UI_Frame", _name);
		_f2.SetBounds(150, 150, 200, 200);
		_f2.Show();
		_tex = _f2.CreateTexture(_texName);
		_tex.PinToBounds();
		_tex.SetTexture(_texture);
		_tex.Show();
		return _f2;
	}

